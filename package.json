{
    "name": "stackblitz",
    "displayName": "STACKBLITZ",
    "description": "StackBlitz - Official Extension",
    "version": "0.0.1",
    "publisher": "stackblitz",
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.stackblitz.open"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.stackblitz.open",
                "title": "Open in StackBlitz"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "@stackblitz/sdk": "^1.1.1",
        "tmp": "0.0.33"
    }
}
